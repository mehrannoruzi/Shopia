@model PagingListDetails<Product>
@{
    ViewBag.Title = $"{Strings.Management} {DomainStrings.Product}";
    Layout = "_LayoutManage";
}
@section styles{
    <link href="@Url.Content("~/Styles/App/Product/manage.css")" rel="stylesheet" />
}

@section filter{
    <partial name="Partials/_Filters" model="new ProductSearchFilter()" />
}
<partial name="Partials/_List" model="@Model" />
@section scripts{
    <script src="@Url.Content("~/Scripts/App/Product/manage.js")"></script>
}