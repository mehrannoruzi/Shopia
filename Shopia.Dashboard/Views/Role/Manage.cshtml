@model PagingListDetails<Role>
@{
    ViewBag.Title = $"{Strings.Management} {DomainStrings.Role}";
    Layout = "_LayoutManage";
}
@section styles{
    <link href="@Url.Content("~/Styles/App/Role/manage.css")" rel="stylesheet" />
}

@section filter{
    <partial name="Partials/_Filters" model="new RoleSearchFilter()" />
}
<partial name="Partials/_List" model="@Model" />
@section scripts{
    <script src="@Url.Content("~/Scripts/App/Role/manage.js")"></script>
}