@model PagingListDetails<Store>
@{
    ViewBag.Title = $"{DomainStrings.Management} {DomainStrings.Store}";
    Layout = "_LayoutManage";
}
@section styles{
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.6.0/dist/leaflet.css"
          integrity="sha512-xwE/Az9zrjBIphAcBb3F6JVqxf46+CDLwfLMHloNu6KEQCAWi6HcDUbeOfBIptF7tcCzusKFjFw2yuvEpDL9wQ=="
          crossorigin="" />
}
@section filter{

    <partial name="Partials/_Filters" model="new StoreSearchFilter()" />
}
<partial name="Partials/_List" model="@Model" />

@section scripts{
    <script src="https://unpkg.com/leaflet@1.6.0/dist/leaflet.js"
            integrity="sha512-gZwIG9x3wUXg2hdXF6+rVkLF/0Vi9U8D2Ntg4Ga5I5BZpVkVxlJWbSQtXPSiUTtC0TjtGOmxa1AJPuV0CPthew=="
            crossorigin=""></script>
    <script src="@Url.Content("~/Scripts/App/02-Store/Store/manage.js")"></script>
}